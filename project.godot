config_version=5

[application]

config/name="Alhambra"
config/description="A procedural generator of things for games."
run/main_scene="res://scenes/game_controller.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="51Z3R"
config/features=PackedStringArray("4.1", "Forward Plus")
boot_splash/bg_color=Color(0.501961, 0, 0.501961, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false

[audio]

buses/default_bus_layout="res://sys/audio_bus_6.tres"

[autoload]

Roller="*res://scenes/dice_roller.tscn"

[debug]

settings/crash_handler/message.editor="Please include this when reporting the bug on: https://github.com/gsizer/godot-4-game/"
file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1600
window/size/viewport_height=900
window/vsync/vsync_mode=0

[editor]

movie_writer/movie_file="f:/gsize/Videos/Recording/gamedev.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/proton_scatter/plugin.cfg", "res://addons/script-tabs/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

